{
  "name": "Github Reviewer Groups",
  "description": "Adds predefined groups of github reviewers",
  "version": "1.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["storage", "activeTab", "scripting"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "/icon.png",
      "32": "/icon.png",
      "48": "/icon.png",
      "128": "/icon.png"
    }
  },
  "icons": {
    "16": "/icon.png",
    "32": "/icon.png",
    "48": "/icon.png",
    "128": "/icon.png"
  }
}
